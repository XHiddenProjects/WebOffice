        <rules>
            <rule>
                <id>1</id>
                <priority>1</priority>
                <action type="redirect">
                    <regexSubstitution>\1</regexSubstitution>
                </action>
                <condition>
                    <regexFilter><![CDATA[()([?&])(utm_source|utm_medium|utm_campaign|ref|fbclid|gclid)=[^&]*]]></regexFilter>
                    <resourceTypes>
                        <type>main_frame</type>
                        <type>sub_frame</type>
