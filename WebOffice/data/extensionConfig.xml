    </adblock>
    <doNotTrack enabled="true">
        <rules>
            <rule id="1">
                <action type="modifyHeaders">
                <requestHeaders>
                    <operation>set</operation>
                    <header>DNT</header>
                    <value>1</value>
                </requestHeaders>
                </action>
                <condition>
                    <resourceTypes>
                        <type>main_frame</type>
                        <type>sub_frame</type>
                        <type>stylesheet</type>
                        <type>script</type>
                        <type>image</type>
                        <type>font</type>
                        <type>object</type>
                        <type>xmlhttprequest</type>
                        <type>ping</type>
                        <type>csp_report</type>
                        <type>media</type>
                        <type>websocket</type>
                        <type>other</type>
                    </resourceTypes>
                </condition>
            </rule>
            <rule id="2" priority="1">
                <action type="modifyHeaders">
                <requestHeaders>
                    <operation>remove</operation>
                    <header>X-Client-Data</header>
                </requestHeaders>
                </action>
                <condition>
                    <resourceTypes>
                        <type>main_frame</type>
                        <type>sub_frame</type>
                        <type>xmlhttprequest</type>
                        <type>other</type>
                    </resourceTypes>
                </condition>
            </rule>
        </rules>
    </doNotTrack>
